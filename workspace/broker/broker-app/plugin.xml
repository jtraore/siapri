<?xml version="1.0" encoding="UTF-8"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="siapri broker">
         <property
               name="appName"
               value="siapri broker">
         </property>
         <property
               name="cssTheme"
               value="broker">
         </property>
         <property
           name="lifeCycleURI"
           value="bundleclass://broker-app/com.siapri.broker.app.ApplicationLifeCycleManager">
         </property>
         <property
               name="startupForegroundColor"
               value="FF0000">
         </property>
         <property
               name="startupMessageRect"
               value="7,252,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="5,275,445,15">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.e4.ui.css.swt.theme">
         <theme label="broker" id="broker" basestylesheeturi="css/default.css" />
   </extension>

</plugin>
